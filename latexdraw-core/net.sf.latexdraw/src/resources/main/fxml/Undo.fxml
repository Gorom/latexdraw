<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.HBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" spacing="2"
      fx:controller="net.sf.latexdraw.instruments.UndoRedoManager">
    <Button fx:id="undoB" mnemonicParsing="false" style="-fx-padding: 5 5 5 5">
        <graphic>
            <ImageView>
                <Image url="@/res/Undo.png"/>
            </ImageView>
        </graphic>
    </Button>
    <Button fx:id="redoB" mnemonicParsing="false" style="-fx-padding: 5 5 5 5">
        <graphic>
            <ImageView>
                <Image url="@/res/Redo.png"/>
            </ImageView>
        </graphic>
    </Button>
</HBox>

