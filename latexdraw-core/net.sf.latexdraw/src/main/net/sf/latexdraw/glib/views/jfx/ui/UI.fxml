<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.sf.latexdraw.glib.views.jfx.ui.Frame">
	<center>
		<TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
			<tabs>
				<Tab closable="false" text="%UIBuilder.1">
					<content>
						<BorderPane>
							<center>
								<SplitPane dividerPositions="0.85" prefHeight="160.0" prefWidth="220.0" BorderPane.alignment="CENTER">
									<items>
										<BorderPane>
											<center>
												<ScrollPane fx:id="scrollPane" BorderPane.alignment="CENTER">
													<content><fx:include fx:id="canvas" source="Canvas.fxml" /></content>
												</ScrollPane>
											</center>
										</BorderPane>
										<BorderPane maxWidth="270.0" minWidth="270.0">
											<center>
												<Accordion maxWidth="270.0" minWidth="270.0" BorderPane.alignment="CENTER">
													<panes>
														<fx:include source="LineStyle.fxml" />
														<fx:include source="FillingStyle.fxml" />
														<fx:include source="ShadowStyle.fxml" />
														<fx:include source="DoubleLineStyle.fxml" />
														<fx:include source="StdGridStyle.fxml" />
														<fx:include source="PlotStyle.fxml" />
														<fx:include source="ArrowStyle.fxml" />
														<fx:include source="TextStyle.fxml" />
														<fx:include source="DotStyle.fxml" />
														<fx:include source="ArcStyle.fxml" />
														<fx:include source="FreehandStyle.fxml" />
														<TitledPane animated="true" text="Shape Transformation">
															<content>
																<AnchorPane>
																	<fx:include source="Rotation.fxml" />
																	<fx:include source="Transformation.fxml" />
																	<fx:include source="Group.fxml" />
																</AnchorPane>
															</content>
														</TitledPane>
														<fx:include source="Dimension.fxml" />
														<fx:include source="Template.fxml" />
													</panes>
												</Accordion>
											</center>
										</BorderPane>
									</items>
								</SplitPane>
							</center>
						</BorderPane>
					</content>
				</Tab>
				<Tab closable="false" text="PST">
					<content>
						<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
					</content>
				</Tab>
				<Tab closable="false" text="%PreferencesFrame.Pref2">
					<content>
						<fx:include source="Preferences.fxml" />
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</center>
	<top>
		<ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<items>
				<MenuButton mnemonicParsing="false" style="-fx-padding: 0 0 0 0">
					<tooltip><Tooltip text="%LaTeXDrawFrame.91" /></tooltip>
					<graphic><ImageView><image><Image url="@/res/New.png" /></image></ImageView></graphic>
					<items>
						<MenuItem id="newMenu" mnemonicParsing="false" text="%Res.2">
							<graphic><ImageView><image><Image url="@/res/New.png" /></image></ImageView></graphic>
						</MenuItem>
						<MenuItem id="loadMenu" mnemonicParsing="false" text="%FileLoaderSaver.3">
							<graphic><ImageView><image><Image url="@/res/document-open.png" /></image></ImageView></graphic>
						</MenuItem>
						<MenuItem id="saveMenu" mnemonicParsing="false" text="%FileLoaderSaver.1">
							<graphic><ImageView><image><Image url="@/res/document-save.png" /></image></ImageView></graphic>
						</MenuItem>
						<MenuItem id="saveAsMenu" mnemonicParsing="false" text="%FileLoaderSaver.2">
							<graphic><ImageView><image><Image url="@/res/document-save-as.png" /></image></ImageView></graphic>
						</MenuItem>
						<Menu id="recentFilesMenu" mnemonicParsing="false" text="%LaTeXDrawFrame.0b" />
					</items>
				</MenuButton>
				<MenuButton mnemonicParsing="false">
					<graphic><ImageView><image><Image url="@/res/Cut.png" /></image></ImageView></graphic>
					<tooltip><Tooltip text="%LaTeXDrawFrame.89" /></tooltip>
					<items>
						<MenuItem id="cutMenu" mnemonicParsing="false" text="%LaTeXDrawFrame.44">
							<graphic><ImageView><image><Image url="@/res/Copy.png" /></image></ImageView></graphic>
						</MenuItem>
						<MenuItem id="pasteMenu" mnemonicParsing="false" text="%LaTeXDrawFrame.43">
							<graphic><ImageView><image><Image url="@/res/Cut.png" /></image></ImageView></graphic>
						</MenuItem>
						<MenuItem id="copyMenu" mnemonicParsing="false" text="%LaTeXDrawFrame.40">
							<graphic><ImageView><image><Image url="@/res/Paste.png" /></image></ImageView></graphic>
						</MenuItem>
					</items>
				</MenuButton>
				<MenuButton id="exportMenu" mnemonicParsing="false">
					<tooltip><Tooltip text="%LaTeXDrawFrame.16b" /></tooltip>
					<graphic><ImageView><image><Image url="@/res/pdf.png" /></image></ImageView></graphic>
					<items>
						<MenuItem id="menuItemPDF" mnemonicParsing="false" text="%Exporter.3" />
						<MenuItem id="menuItemPDFcrop" mnemonicParsing="false" text="%Exporter.4" />
						<MenuItem id="menuItemEPSLatex" mnemonicParsing="false" text="%Exporter.2" />
						<MenuItem id="menuItemPST" mnemonicParsing="false" text="%LaTeXDrawFrame.19" />
						<MenuItem id="menuItemJPG" mnemonicParsing="false" text="%LaTeXDrawFrame.20" />
						<MenuItem id="menuItemPNG" mnemonicParsing="false" text="%LaTeXDrawFrame.7" />
						<MenuItem id="menuItemBMP" mnemonicParsing="false" text="%LaTeXDrawFrame.6" />
						<MenuItem id="exportTemplateMenu" mnemonicParsing="false" text="%LaTeXDrawFrame.42" />
					</items>
				</MenuButton>
				<Spinner min="10" max="450" initialValue="100" amountToStepBy="5" prefHeight="25.0" prefWidth="75.0" />
				<Button fx:id="deleteB" mnemonicParsing="false" style="-fx-padding: 5 5 5 5">
					<graphic><ImageView><image><Image url="@/res/del.png" /></image></ImageView></graphic>
				</Button>
				<HBox spacing="10" />
				<fx:include source="EditingModes.fxml" />
				<fx:include source="Error.fxml" />
				<fx:include source="Help.fxml" />
			</items>
		</ToolBar>
	</top>
</BorderPane>
