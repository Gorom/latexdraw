<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>

<MenuButton mnemonicParsing="false" style="-fx-padding: 0 0 0 0"
			xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
			fx:controller="net.sf.latexdraw.instruments.FileLoaderSaver">
	<tooltip><Tooltip text="%LaTeXDrawFrame.91" /></tooltip>
	<graphic><ImageView><image><Image url="@/res/New.png" /></image></ImageView></graphic>
	<items>
		<MenuItem fx:id="newMenu" mnemonicParsing="false" text="%Res.2">
			<graphic><ImageView><image><Image url="@/res/New.png" /></image></ImageView></graphic>
		</MenuItem>
		<MenuItem fx:id="loadMenu" mnemonicParsing="false" text="%FileLoaderSaver.3">
			<graphic><ImageView><image><Image url="@/res/document-open.png" /></image></ImageView></graphic>
		</MenuItem>
		<MenuItem fx:id="saveMenu" mnemonicParsing="false" text="%FileLoaderSaver.1">
			<graphic><ImageView><image><Image url="@/res/document-save.png" /></image></ImageView></graphic>
		</MenuItem>
		<MenuItem fx:id="saveAsMenu" mnemonicParsing="false" text="%FileLoaderSaver.2">
			<graphic><ImageView><image><Image url="@/res/document-save-as.png" /></image></ImageView></graphic>
		</MenuItem>
		<Menu fx:id="recentFilesMenu" mnemonicParsing="false" text="%LaTeXDrawFrame.0b" />
	</items>
</MenuButton>
