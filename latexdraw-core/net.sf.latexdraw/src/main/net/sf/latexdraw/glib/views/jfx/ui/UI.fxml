<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="800.0" prefWidth="1200.0" fx:controller="net.sf.latexdraw.glib.views.jfx.ui.Frame" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
   <top>
      <MenuBar fx:id="menuBar" BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <SplitPane dividerPositions="0.85" prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <items>
                  <BorderPane>
                     <center>
                        <ScrollPane fx:id="scrollPane" BorderPane.alignment="CENTER">
                           <content>
                              <fx:include fx:id="canvas" source="Canvas.fxml" />
                           </content>
                        </ScrollPane>
                     </center>
                  </BorderPane>
                  <BorderPane minWidth="250.0" maxWidth="250.0">
                     <center>
                        <Accordion BorderPane.alignment="CENTER" minWidth="250.0" maxWidth="250.0">
                          <panes>
                            <TitledPane animated="false" text="Templates">
                              <content>
                                <AnchorPane/>
                              </content>
                            </TitledPane>
                            <TitledPane animated="false" text="Shape Transformation">
                              <content>
                                <AnchorPane/>
                              </content>
                            </TitledPane>
                            <TitledPane animated="false" text="Line style">
                              <content>
								<fx:include source="LineStyle.fxml" />
                              </content>
                            </TitledPane>
                          </panes>
                        </Accordion>
                     </center>
                  </BorderPane>
               </items>
            </SplitPane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
