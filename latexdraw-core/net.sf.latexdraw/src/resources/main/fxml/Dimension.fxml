<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPane" animated="true" text="Position / Dimensions"
            xmlns="http://javafx.com/javafx/8"
            fx:controller="net.sf.latexdraw.instruments.ShapeCoordDimCustomiser">
    <AnchorPane minWidth="250" maxWidth="250" layoutX="10.0" layoutY="5.0"
                style=" -fx-border-color:lightgray; -fx-border-width: 1; -fx-border-style: solid; -fx-border-radius: 5; -fx-border-insets: -3;">
        <Label layoutX="0.0" layoutY="9.0" text="x"/>
        <Spinner fx:id="tlxS" prefHeight="25.0" prefWidth="85.0" layoutX="40.0" layoutY="6.0" initialValue="0.0" amountToStepBy="1.0"
                 min="-100000" max="100000">
            <tooltip>
                <Tooltip text="ShapeCoord.1"/>
            </tooltip>
        </Spinner>
        <Label layoutX="0.0" layoutY="39.0" text="y"/>
        <Spinner fx:id="tlyS" prefHeight="25.0" prefWidth="85.0" layoutX="40.0" layoutY="36.0" initialValue="0.0" amountToStepBy="1.0"
                 min="-100000" max="100000">
            <tooltip>
                <Tooltip text="ShapeCoord.2"/>
            </tooltip>
        </Spinner>
    </AnchorPane>
</TitledPane>
